<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitness">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/running"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginOrSignUpActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".MainActivity"
            android:label="Fitness"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".EnterDataActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".AccountSettingsActivity" />
        <activity android:name=".ProfilePicActivity"></activity>
    </application>

</manifest>